{
  "compilerOptions": {
    "outDir": "dist",
    "module": "ESNext",
    "target": "ES5", // 编译成es的哪个版本
    "declaration": true, // 为每个ts文件生成d.ts文件
    "jsx":"react",
    "moduleResolution": "Node",
    "allowSyntheticDefaultImports": true
  },
  "include": [
    "src"
  ],
  "exclude": [
    "src/**/*.test.tsx",
    "src/**/*.stories.tsx"
  ]
}
